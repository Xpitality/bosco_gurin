{
   "executionRoleArn":"arn:aws:iam::934187374409:role/ecsTaskExecutionRole",
   "containerDefinitions":[
      {
         "logConfiguration":{
            "logDriver":"awslogs",
            "options":{
               "awslogs-group":"/ecs/bosco-gurin-app",
               "awslogs-region":"eu-central-1",
               "awslogs-stream-prefix":"ecs"
            }
         },
         "portMappings":[
            {
               "hostPort":3000,
               "protocol":"tcp",
               "containerPort":3000
            }
         ],
         "environment":[
            {
               "name":"NODE_ENV",
               "value":"production"
            },
            {
               "name":"RAILS_ENV",
               "value":"production"
            },
            {
               "name":"RAILS_LOG_TO_STDOUT",
               "value":"true"
            },
            {
               "name":"RAILS_SERVE_STATIC_FILES",
               "value":"true"
            }
         ],
         "secrets":[
            {
               "valueFrom":"Bosco-Gurin-App-Rails-Master-Key",
               "name":"RAILS_MASTER_KEY"
            }
         ],
         "memory":512,
         "memoryReservation":256,
         "image":"934187374409.dkr.ecr.eu-central-1.amazonaws.com/gurin:app-production",
         "essential":true,
         "name":"BoscoGurinApp"
      }
   ],
   "memory":"512",
   "taskRoleArn":"arn:aws:iam::934187374409:role/ecsTaskExecutionRole",
   "family":"bosco-gurin-app-task",
   "requiresCompatibilities":[
      "FARGATE"
   ],
   "networkMode":"awsvpc",
   "cpu":"256",
   "tags":[
      {
         "key":"client",
         "value":"bosco-gurin"
      }
   ]
}
